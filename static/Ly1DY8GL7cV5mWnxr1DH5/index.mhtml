t; }
  .container { min-width: 0px; }
  #header { margin-bottom: 0px !important; }
  .contentWrapper { padding: 0px !important; }
  #contentinfo { text-align: left; }
  #contentinfo li { display: none; }
  #contentinfo li.last { display: block; }
  #contentinfo li.last span { margin: 0px; }
  h2, h3, img, ul { break-after: avoid; }
  #content { min-height: 0px !important; }
