  .publications.period .opened a.collapser { font-weight: 700 !important; }
  .carousel { display: none; }
  table { break-inside: avoid; }
  .form, form .boxed { background: transparent; border: none !important; }
  form button { display: none; }
  #lightboxControls, .lightboxOverlay { display: none !important; }
  #lightbox { position: relative; left: auto !important; top: auto !importa=
nt; }
