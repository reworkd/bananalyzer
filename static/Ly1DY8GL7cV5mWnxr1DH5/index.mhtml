  #content #footnotes.hidden { display: block !important; }
  #content #footnotes ul { list-style: outside none decimal; margin-left: 3=
0px; }
  a::before { background: none !important; height: 0px !important; width: 0=
px !important; }
  figcaption, figure img { max-width: 18cm; }
  .glossary, .glossary-details .headingwrapper a, .glossary-results, .quick=
view, .section.small-spacing, .teaserbox .playbutton { display: none; }
  #content .notes li, .contact .contactbox, .pictures .teaserWrapper, .publ=
ication.column, .publications.period li, .teaserbox, .videos .teaserbox { b=
reak-inside: avoid; }
  .filterbox { border: none; }
  .filterbox form { padding: 0px; }
  .filterbox form .row.last button { display: none; }
  .fluidContent .collapser, .fluidContent .collapser h2, .fluidContent .col=
lapser h3, .fluidContent .collapser h4 { padding-left: 0px; }
  .fluidContent .collapser::before { background: transparent; height: 0px; =
width: 0px; }
  .glossary-details .headingwrapper { break-after: avoid; }
  .glossary-details .description { break-before: avoid; }
  .glossary-details h3 { font-size: 1.5em; }
